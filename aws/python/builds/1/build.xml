                }
            }
        }

        stage(&apos;Git Checkout&apos;) {
            steps {
                git branch: &apos;master&apos;,
                    url: &apos;https://github.com/CharanSai8127/full-stack-fastapi-template.git&apos;
            }
        }

        stage(&apos;Prepare Workspace&apos;) {
            steps {
                sh &apos;&apos;&apos;
                  mkdir -p ${REPORTS_DIR}/owasp ${REPORTS_DIR}/trivy
                  rm -rf /var/lib/jenkins/.cache/trivy || true
                &apos;&apos;&apos;
            }
        }

        stage(&apos;OWASP Dependency Check&apos;) {
            steps {
                sh &apos;&apos;&apos;
                  set +e

                  pipx install safety || pip install --user safety
                  safety check --full-report \
                    --output ${REPORTS_DIR}/owasp/safety-${TIMESTAMP}.json || true

                  cd frontend
                  npm audit --json &gt; ../${REPORTS_DIR}/owasp/npm-audit-${TIMESTAMP}.json || true
                  cd ..

                  set -e
                &apos;&apos;&apos;
            }
        }

        stage(&apos;Build Backend Image&apos;) {
            steps {
                sh &apos;&apos;&apos;
                  docker pull ${DOCKER_HUB_USER}/backend:${IMAGE_TAG} || true
                  docker build --pull \
                    --cache-from ${DOCKER_HUB_USER}/backend:${IMAGE_TAG} \
                    -t ${DOCKER_HUB_USER}/backend:${IMAGE_TAG} ./backend
                &apos;&apos;&apos;
            }
        }

        stage(&apos;Build Frontend Image&apos;) {
            steps {
                sh &apos;&apos;&apos;
                  docker pull ${DOCKER_HUB_USER}/frontend:${IMAGE_TAG} || true
                  docker build --pull \
                    --cache-from ${DOCKER_HUB_USER}/frontend:${IMAGE_TAG} \
                    -t ${DOCKER_HUB_USER}/frontend:${IMAGE_TAG} ./frontend
                &apos;&apos;&apos;
            }
        }

        stage(&apos;Trivy Image Scan (Safe Mode)&apos;) {
            steps {
                sh &apos;&apos;&apos;
                  trivy image \
                    --scanners vuln \
                    --skip-db-update \
                    --no-progress \
                    --format json \
                    --output ${REPORTS_DIR}/trivy/backend-${TIMESTAMP}.json \
                    ${DOCKER_HUB_USER}/backend:${IMAGE_TAG} || true

                  trivy image \
                    --scanners vuln \
                    --skip-db-update \
                    --no-progress \
                    --format json \
                    --output ${REPORTS_DIR}/trivy/frontend-${TIMESTAMP}.json \
                    ${DOCKER_HUB_USER}/frontend:${IMAGE_TAG} || true
                &apos;&apos;&apos;
            }
        }

        stage(&apos;Push Docker Images&apos;) {
            steps {
                withCredentials([
                    usernamePassword(
                        credentialsId: &apos;docker-cred&apos;,
                        usernameVariable: &apos;DOCKER_USER&apos;,
                        passwordVariable: &apos;DOCKER_PASS&apos;
                    )
                ]) {
                    sh &apos;&apos;&apos;
                      echo &quot;$DOCKER_PASS&quot; | docker login \
                        -u &quot;$DOCKER_USER&quot; --password-stdin

                      docker push ${DOCKER_HUB_USER}/backend:${IMAGE_TAG}
                      docker push ${DOCKER_HUB_USER}/frontend:${IMAGE_TAG}

                      docker logout
                    &apos;&apos;&apos;
                }
            }
        }
    }

